{%  extends 'base.html.twig'%}
{% block title %}
    Kontakt
{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" href="{{ asset('css/trackInfo/index.css') }}">
{% endblock %}
{% block body %}
    {{ parent() }}
    <div class="loader"></div>
    <div class="container">
        <div class="col-md-12 alertArea">
        </div>
        <h4 class="pageHeader">Kontakt</h4>
        <div class="row">
            <div class="col-md-6 contactArea">
                <div class="card">
                    <div class="card-body">
                        <ul>
                            <li class="d-flex justify-content-between align-items-center"><i class="fas fa-map-marker-alt"></i><span class="contact-data">ul.Rojna 53, Łódź</span></li>
                            <li class="d-flex justify-content-between align-items-center"><i class="far fa-envelope"></i><span class="contact-data">aa@gmail.com</span></li>
                            <li class="d-flex justify-content-between align-items-center"><i class="fas fa-phone-volume"></i><span class="contact-data">505-100-200</span></li>
                            <li class="d-flex justify-content-between align-items-center"><i class="far fa-clock"></i><span class="contact-data">14:00 - 21:00</span></li>
                            <li class="d-flex justify-content-between align-items-center"><i class="fab fa-facebook-square"></i><a href="#" class="contact-data">fanpgae</a></li>
                            <li class="d-flex justify-content-between align-items-center"><i class="fab fa-instagram"></i><a href="#" class="contact-data">instagram</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="card">
                    <div class="card-body">
                        <div id="map">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
{%  endblock %}
